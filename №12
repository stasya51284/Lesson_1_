public class Main {
    public static void multiplyIfLessThanSix(int[] arr) {
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] < 6) {
                arr[i] *= 2;
            }
        }
    }

    public static void main(String[] args) {
        int[] array = {1, 5, 3, 7, 2, 8, 4};
        multiplyIfLessThanSix(array);
        for (int i : array) {
            System.out.print(i + " "); 
        }
    }
}
