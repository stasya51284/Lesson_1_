public class Main {
    public static void invertArray(int[] arr) {
        for (int i = 0; i < arr.length; i++) {
            arr[i] = (arr[i] == 0) ? 1 : 0;
        }
    }

    public static void main(String[] args) {
        int[] array = {1, 0, 1, 0, 1, 0};
        invertArray(array);
        for (int i : array) {
            System.out.print(i + " "); // Output: 0 1 0 1 0 1
        }
    }
}
