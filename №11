public class Main {
    public static void fillArray(int[] arr) {
        for (int i = 0; i < arr.length; i++) {
            arr[i] = i + 1;
        }
    }

    public static void main(String[] args) {
        int[] array = new int[10];
        fillArray(array);
        for (int i : array) {
            System.out.print(i + " "); 
        }
    }
}
